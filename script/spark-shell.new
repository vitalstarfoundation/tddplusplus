#!/bin/bash
cd "$(dirname ${BASH_SOURCE[0]})"/../target/scala-2.11
if [ ! -f tddplusplus-assembly-0.0.1.jar ]; then
    /usr/local/bin/sbt assembly
fi
cd "$(dirname ${BASH_SOURCE[0]})"/../tmp
spark222/bin/spark-shell --conf "spark.driver.extraClassPath=../target/scala-2.11/classes" --jars `ls ../target/scala-2.11/*assembly*.jar`;
